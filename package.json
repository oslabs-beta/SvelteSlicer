{
  "name": "svelte-app",
  "version": "1.0.0",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "babel-jest": "^27.4.6",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "jest": "^27.4.7",
    "mini-css-extract-plugin": "^1.3.4",
    "svelte": "^3.31.2",
    "svelte-jester": "^2.1.5",
    "svelte-loader": "^3.0.0",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "dev": "webpack serve --content-base public",
    "test": "jest src",
    "test:watch": "npm run test -- --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "svelte-jester",
      "setupFilesAfterEnv": ["@testing-library/jest-dom/extend-expect"]
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ]
  },
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "dependencies": {
    "d3": "^7.2.1",
    "lodash": "^4.17.21"
  }
}
