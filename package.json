{
  "name": "svelte_slicer",
  "version": "1.0.0",
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@happy-dom/global-registrator": "^8.1.1",
    "@happy-dom/jest-environment": "^8.1.1",
    "babel-jest": "^29.3.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-svelte3": "^3.4.0",
    "file-loader": "^6.2.0",
    "happy-dom": "^8.1.1",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^1.3.4",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.31.2",
    "svelte-jester": "^2.3.2",
    "svelte-loader": "^3.0.0",
    "webpack": "^5.16.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "scripts": {
    "build-client": "cross-env NODE_ENV=production webpack --config webpack.config.client.js",
    "build-script": "cross-env webpack --config webpack.config.script.js",
    "build-all": "npm run build-client && npm run build-script",
    "watch-client": "cross-env NODE_ENV=development webpack --config webpack.config.client.js --watch",
    "watch-script": "cross-env webpack --config webpack.config.script.js --watch",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "check-format": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.svelte --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "d3": "^7.2.1",
    "lodash": "^4.17.21"
  }
}
